{
  "name": "vue3-admin-template-v2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode development --open",
    "dev:test": "vite --mode test",
    "build": "run-p type-check build-only --mode development",
    "build:test": "run-p type-check build-only --mode test",
    "build:prod": "run-p type-check build-only  --mode production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "lint:eslint": "eslint src/**/*.{ts,vue} --cache --fix",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --cache --fix",
    "prepare": "husky install",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "preinstall": "node ./scripts/preinstall.js"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.0",
    "dayjs": "^1.11.10",
    "jsbarcode": "^3.11.5",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "qrcode": "^1.5.3",
    "query-string": "^8.1.0",
    "sortablejs": "^1.15.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-waterfall-plugin-next": "^2.2.4"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.51.1",
    "@arco-plugins/vite-vue": "^1.4.5",
    "@babel/eslint-parser": "^7.22.15",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.198",
    "@types/node": "^18.17.5",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.5.2",
    "@types/sortablejs": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "icon": "link:@arco-design/web-vue/es/icon",
    "jsdom": "^22.1.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-less": "^2.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-less": "^2.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-less": "^2.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-mock": "^2.9.6",
    "vitest": "^0.34.2",
    "vue-tsc": "^1.8.8"
  }
}
